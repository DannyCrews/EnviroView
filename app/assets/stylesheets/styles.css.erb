body { 
  height: 100%;
  width: 1000; 
  margin: 50; 
  padding: 50;
  background-image: url(<%= asset_path('img/clouds.png') %>);
  background-color: #68e5ff;
  text-align: center;
}

.title {
  padding: 40px;
}

.visualization {
  height: 700px;
  width: 1000px;
  padding: 20px;
  margin: 40px;
  margin: auto;
}

.info {
  padding: 6px 8px;
  font: 14px/16px Arial, Helvetica, sans-serif;
  background: white;
  background: rgba(255, 255, 255, 0.8);
  box-shadow: 0 0 15px rgba(0,0,0,0.2);
  border-radius: 5px;
}

.info h4 {
  margin: 0 0 5px;
  color: #777;
}

#banner {
  height: 200px;
  width: 100%;
  padding: 40px;
}

#banner h1 {
  font-family: 'Cantata One', serif;
  font-size: 100px;
}

#googft-legend    {
  background-color:#fff; border:1px solid #000;
  font-family:Arial, sans-serif;
  font-size:12px;
  margin:5px;
  padding:10px 10px 8px;
}

#googft-legend p  {
  font-weight:bold;
    margin-top:0;
}

#googft-legend div  {
  margin-bottom:5px;
}

.googft-legend-swatch {
  border:1px solid;
  float:left;
  height:12px;
  margin-right:8px;
  width:20px;
}

.googft-legend-range  {
  margin-left:0;
}

.googft-dot-icon  {
  margin-right:8px;
}

.googft-paddle-icon {
  height:24px;
  left:-8px;
  margin-right:-8px;
  position:relative;
  vertical-align:middle;
  width:24px;
}

.googft-legend-source {
  margin-bottom:0;
  margin-top:8px;
}

.googft-legend-source a {
  color:#666;
  font-size:11px;
}

